<service>
	<id>noderfc.exe</id>
	<name>NodeRFC</name>
	<description>The nodejs connect to RFC.</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>C:\Users\Win7\AppData\Roaming\npm\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>D:\node\AppRFC\app.js</argument>
	<argument>--log</argument>
	<argument>NodeRFC wrapper</argument>
	<argument>--grow</argument>
	<argument>0.5</argument>
	<argument>--wait</argument>
	<argument>2</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<env name="NODE_ENV" value="production"/>
	<workingdirectory>D:\node\AppRFC</workingdirectory>
</service>